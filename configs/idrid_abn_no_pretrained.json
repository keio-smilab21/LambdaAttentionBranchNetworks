{
    "seed": 42,
    "model": "resnet50",
    "add_attention_branch": true,
    "div": "layer2",
    "trainable_bottleneck": 2,
    "dataset": "IDRiD",
    "image_size": 224,
    "batch_size": 32,
    "train_ratio": 0.8,
    "loss_weights": [
        2.0,
        1.0
    ],
    "epochs": 300,
    "optimizer": "AdamW",
    "lr": 0.1,
    "lr_linear": 0.1,
    "lr_ab": 0.1,
    "min_lr": 1e-6,
    "weight_decay": 0.09,
    "factor": 0.333,
    "sceduler_patience": 2,
    "early_stopping_patience": 300
}