{
    "seed": 42,
    "model": "resnet50",
    "add_attention_branch": false,
    "base_pretrained": "pytorch default",
    "trainable_bottleneck": 2,
    "dataset": "IDRiD",
    "image_size": 224,
    "batch_size": 32,
    "train_ratio": 0.8,
    "epochs": 300,
    "optimizer": "AdamW",
    "lr": 1e-4,
    "lr_linear": 1e-3,
    "min_lr": 1e-6,
    "weight_decay": 0.09,
    "factor": 0.333,
    "sceduler_patience": 2,
    "early_stopping_patience": 6
}