{
    "seed": 42,
    "no_deterministic": true,
    "model": "lambda_resnet",
    "add_attention_branch": true,
    "div": "layer2",
    "base_pretrained2": "checkpoints/idrid_lambda-base.pt",
    "freeze": [
        "fe",
        "perception"
    ],
    "trainable_bottleneck": 0,
    "dataset": "IDRiD",
    "image_size": 224,
    "batch_size": 32,
    "train_ratio": 0.8,
    "loss_weights": [
        2.0,
        1.0
    ],
    "epochs": 300,
    "optimizer": "AdamW",
    "lr": 0.0001,
    "lr_linear": 0.001,
    "lr_ab": 0.001,
    "min_lr": 1e-06,
    "momentum": 0.9,
    "weight_decay": 0.09,
    "factor": 0.333,
    "scheduler_patience": 2,
    "early_stopping_patience": 6,
    "save_dir": "checkpoints",
    "sceduler_patience": 2,
    "lambda_att": 1,
    "lambda_var": 0
}