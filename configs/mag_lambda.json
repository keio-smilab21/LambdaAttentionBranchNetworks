{
    "seed": 42,
    "model": "CNN",
    "add_attention_branch": false,
    "trainable_bottleneck": 5,
    "dataset": "magnetogram",
    "image_size": 512,
    "batch_size": 32,
    "train_ratio": 0.8,
    "loss_weights": [
        1.0
    ],
    "epochs": 300,
    "optimizer": "AdamW",
    "lr": 1e-4,
    "lr_linear": 1e-3,
    "lr_ab": 0.1,
    "min_lr": 1e-6,
    "weight_decay": 0.09,
    "factor": 0.333,
    "sceduler_patience": 2,
    "early_stopping_patience": 6,
    "lambda_att": 0,
    "lambda_var": 0
}